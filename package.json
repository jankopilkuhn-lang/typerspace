{
  "name": "typer-space",
  "version": "1.0.0",
  "description": "A game development project using TypeScript and Phaser",
  "main": "dist/index.js",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production && copyfiles -f public/index.html dist",
    "proxy": "node server.js",
    "start": "concurrently \"npm run proxy\" \"npm run dev\"",
    "start:frontend": "npm run dev",
    "start:proxy": "npm run proxy"
  },
  "keywords": [
    "game",
    "phaser",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "dotenv": "^17.2.4",
    "dotenv-webpack": "^8.1.1",
    "file-loader": "^6.2.0",
    "phaser-ce": "^2.20.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@types/three": "^0.182.0",
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "phaser": "2.6.2",
    "three": "^0.182.0"
  }
}
